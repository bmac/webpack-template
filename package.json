    "build-dist": "webpack --mode production --devtool eval-cheap-module-source-map --config webpack.config.js",
    "build-watch": "webpack ./js/main.js --mode development --watch --devtool eval-cheap-module-source-map",
    "serve": "webpack-dev-server --mode development --devtool eval-cheap-module-source-map --config webpack.config.js --output-public-path /dist/ --static public .",
